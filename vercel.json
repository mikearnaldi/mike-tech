{
  "buildCommand": "bun run build",
  "outputDirectory": "dist",
  "framework": "astro",
  "rewrites": [
    {
      "source": "/a/static/:path*",
      "destination": "https://us-assets.i.posthog.com/static/:path*"
    },
    {
      "source": "/a/:path(.*)",
      "destination": "https://us.i.posthog.com/:path"
    }
  ]
}
